{
    "name" : "mild/mild",
    "keywords": ["framework", "mild"],
    "type": "project",
    "description": "Mild is a PHP Framework that is fast and easy to learn and develop",
    "authors": [
        {
            "name": "Mochammad Riyadh Ilham Akbar Pasya",
            "email": "ilhampasya220317@gmail.com"
        }
    ],
    "autoload": {
        "files" : [
            "Mild/Supports/helpers.php"
        ],
        "psr-4": {
            "App\\" : "app/",
            "Mild\\" : "Mild/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "nesbot/carbon": "^2.11",
        "psr/http-message": "^1.0",
        "psr/log": "^1.1",
        "psy/psysh": "^0.9.9",
        "symfony/console": "^4.2"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "nunomaduro/collision": "^2.1",
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
